{
  "builders": [{
    "type": "qemu",
    "iso_url": "{{user `iso_url`}}",
    "iso_checksum": "{{user `iso_checksum`}}",
    "iso_checksum_type": "{{user `iso_checksum_type`}}",
    "output_directory": "output-fedora-rawhide-x86_64-{{build_type}}",
    "vm_name": "packer-fedora-rawhide-x86_64",
    "disk_size": "{{user `disk_size`}}",
    "headless": "{{user `headless`}}",
    "http_directory": "http",
    "boot_wait": "5s",
    "boot_command": [
      "<tab> ",
      "inst.ks=http://{{.HTTPIP}}:{{.HTTPPort}}/fedora-rawhide/anaconda-ks.cfg ",
      "biosdevname=0 ",
      "net.ifnames=0 ",
      "<enter>"
    ],
    "ssh_timeout": "{{user `ssh_timeout`}}",
    "ssh_username": "vagrant",
    "ssh_password": "vagrant",
    "shutdown_command": "sudo systemctl poweroff",
    "qemuargs": [
      ["-m", "{{user `memory`}}"],
      ["-smp", "{{user `cpus`}}"]
    ]
  }, {
    "type": "virtualbox-iso",
    "guest_os_type": "Fedora_64",
    "iso_url": "{{user `iso_url`}}",
    "iso_checksum": "{{user `iso_checksum`}}",
    "iso_checksum_type": "{{user `iso_checksum_type`}}",
    "output_directory": "output-fedora-rawhide-x86_64-{{build_type}}",
    "vm_name": "packer-fedora-rawhide-x86_64",
    "disk_size": "{{user `disk_size`}}",
    "headless": "{{user `headless`}}",
    "http_directory": "http",
    "boot_wait": "5s",
    "boot_command": [
      "<tab> ",
      "inst.ks=http://{{.HTTPIP}}:{{.HTTPPort}}/fedora-rawhide/anaconda-ks.cfg ",
      "biosdevname=0 ",
      "net.ifnames=0 ",
      "<enter>"
    ],
    "ssh_timeout": "{{user `ssh_timeout`}}",
    "ssh_username": "vagrant",
    "ssh_password": "vagrant",
    "shutdown_command": "sudo systemctl poweroff",
    "vboxmanage": [
      ["modifyvm", "{{.Name}}", "--memory", "{{user `memory`}}"],
      ["modifyvm", "{{.Name}}", "--cpus", "{{user `cpus`}}"]
    ]
  }, {
    "type": "vmware-iso",
    "guest_os_type": "fedora-64",
    "iso_url": "{{user `iso_url`}}",
    "iso_checksum": "{{user `iso_checksum`}}",
    "iso_checksum_type": "{{user `iso_checksum_type`}}",
    "output_directory": "output-fedora-rawhide-x86_64-{{build_type}}",
    "vm_name": "packer-fedora-rawhide-x86_64",
    "disk_size": "{{user `disk_size`}}",
    "headless": "{{user `headless`}}",
    "http_directory": "http",
    "boot_wait": "5s",
    "boot_command": [
      "<tab> ",
      "inst.ks=http://{{.HTTPIP}}:{{.HTTPPort}}/fedora-rawhide/anaconda-ks.cfg ",
      "biosdevname=0 ",
      "net.ifnames=0 ",
      "<enter>"
    ],
    "ssh_timeout": "{{user `ssh_timeout`}}",
    "ssh_username": "vagrant",
    "ssh_password": "vagrant",
    "shutdown_command": "sudo systemctl poweroff",
    "vmx_data": {
      "memsize": "{{user `memory`}}",
      "numvcpus": "{{user `cpus`}}"
    }
  }],
  "provisioners": [{
    "type": "shell",
    "scripts": [
      "scripts/fedora/virtualbox.sh",
      "scripts/fedora/vmware.sh",
      "scripts/common/vagrant.sh",
      "scripts/common/sshd.sh",
      "scripts/fedora/cleanup.sh",
      "scripts/common/minimize.sh"
    ]
  }],
  "post-processors": [{
    "type": "vagrant",
    "compression_level": "{{user `compression_level`}}",
    "output": "fedora-rawhide-x86_64-{{.Provider}}.box"
  }],
  "variables": {
    "compression_level": "6",
    "cpus": "1",
    "disk_size": "40000",
    "headless": "false",
	"iso_url": "http://download.fedoraproject.org/pub/fedora/linux/development/rawhide/Server/x86_64/iso/Fedora-Server-netinst-x86_64-Rawhide-20180321.n.0.iso",
	"iso_checksum": "9a67d33bd5f5ecda032c7f4f74bbfb702b0837b68b853d9cd752056ca940d669",
    "iso_checksum_type": "sha256",
    "memory": "2048",
    "ssh_timeout": "60m",
    "qcow2_checksum": "",
    "qcow2_checksum_type": "sha256"
  }
}
